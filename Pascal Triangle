// Time Complexity:- O(n)
// Space Complexity:- O(n), to store the result.
class Solution:

	def nthRowOfPascalTriangle(self, n):
        ans = [1]
        result = 1
        
        for i in range (1, n):
            result = result * (n - i)
            result = result // i
            ans.append(result)
            
        return ans;
