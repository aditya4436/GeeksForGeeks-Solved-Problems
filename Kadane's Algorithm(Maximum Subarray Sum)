#Time complexity:- O(n)
# Space complexity:- O(1)
class Solution:
    def maxSubarraySum(self, arr):
        n = len(arr)
        sum = 0
        maximumSum = -10**100
        
        for i in range(n):
            sum += arr[i]
            maximumSum = max(maximumSum, sum)
            if sum < 0:
                sum = 0
        
        return maximumSum
